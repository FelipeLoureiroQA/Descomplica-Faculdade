algoritmo "Parte 2 - Enquanto"
var
    numeros : inteiro
    i : inteiro
    resultado : inteiro
inicio
i <- 1
resultado <- 1
    enquanto(i <= 3) faca
        escreva("Digite um numero: ")
        leia(numeros)
        resultado <- (numeros * resultado)
         i <- i + 1
         escreval("O resultado Ã©: ", resultado)
    fimenquanto
fimalgoritmo