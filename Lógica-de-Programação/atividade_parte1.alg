algoritmo "Parte 1 - Para"
var
    numeros : inteiro
    i : inteiro
    resultado : inteiro
inicio
    resultado <- 1
    para i <- 1 ate 2 faca
        escreva("Digite um numero: ")
    leia(numeros)
        resultado <- resultado * numeros        
        escreval(resultado)
    fimpara

fimalgoritmo